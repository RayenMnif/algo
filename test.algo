procedure remplir(tab : tableau, n : entier)
debut 
   
   # lire n (n is being auto allocataed in memory)
   
   repeter
       n <- lire("lire n: ")
       n <- valeur(n) # returns a float
       n <- ent(n)
    jusqu'a n >= 3

   # lire tab 
   
   pour i de 0 a n - 1 faire
       lire(tab[i])
   fin_pour 
   
fin

procedure afficher(tab: tableau, n: entier)
debut
    i <- 0
    tant_que i < n faire
       ecrire(tab[i])
       i <- i + 1
    fin_tant_que
fin 

remplir(T, n)
afficher(T, n)

